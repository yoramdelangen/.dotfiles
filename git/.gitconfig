[user]
	email = yoramdelangen@gmail.com
	name = Yoram de Langen
[core]
	quotepath = false
	excludesfile = /Users/Yoram/.gitignore_global
	editor = vim
	autocrlf = false
[push]
	default = matching
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
[init]
	defaultBranch = main
